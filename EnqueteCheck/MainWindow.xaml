<Window x:Class="EnqueteCheck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button 
            Click="OnClickCheck"
            Content="Check" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox 
            x:Name="textID"
            HorizontalAlignment="Left" Height="23" Margin="90,11,0,0" TextWrapping="Wrap" Text="bZv0nvY" VerticalAlignment="Top" Width="120"/>
        <TextBox 
            x:Name="textOut"
            AcceptsReturn="True" Margin="10,10,10,46" Grid.Row="1" TextWrapping="Wrap" Text="TextBox"/>
        <TextBox 
            x:Name="textError"
            Margin="10,0,10,10" Grid.Row="1" TextWrapping="Wrap" Text="TextBox" Height="23" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
